//problem link: https://onlinejudge.org/index.php?option=onlinejudge&Itemid=8&page=show_problem&problem=4396

#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define PB push_back
#define sz(x) (int)x.size()
#define all(x) x.begin(),x.end()
#define F first
#define S second

void solve(){
	int n; cin >> n;
	string arr[10];
	for(int i = 0; i < 5; i++){
		cin >> arr[i];
	}
	for(int i = 0; i < 4*n; i+=4){
		if(arr[0][i]=='.')cout<<1;
		else if(arr[3][i]=='*')cout<<2;
		else cout<<3;
	}
	cout<<'\n';
}

int main(){
  ios_base::sync_with_stdio(false), cout.tie(NULL), cin.tie(NULL);
	int testcase = 1;
	//cin >> testcase;
	while(testcase--){
		solve();
	}
} 	 		   			  	
